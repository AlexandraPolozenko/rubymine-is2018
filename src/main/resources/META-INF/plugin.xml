<idea-plugin>
  <id>org.intellij.is2018.python.plugin</id>
  <name>Rubymine-SI2018</name>
  <version>1.0</version>
  <vendor url="https://jetbrains.com">JetBrains</vendor>
  <description>Template project for writing inspections which finds constant expressions</description>
  <change-notes/>

  <depends>com.intellij.modules.lang</depends>
  <depends>com.intellij.modules.python</depends>

  <extensions defaultExtensionNs="com.intellij">
    <localInspection language="Python" shortName="PyConstantExpression"
                     enabledByDefault="true" level="WARNING" suppressId="PyConstantExpression"
                     implementationClass="com.jetbrains.python.inspection.PyConstantExpression"
                     displayName="Constant expression inspection"/>
  </extensions>

</idea-plugin>